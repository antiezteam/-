		Background.Parent = ScreenGui

		-- –û—Å–Ω–æ–≤–Ω–æ–µ –æ–∫–Ω–æ –≤—ã–±–æ—Ä–∞ –∏–≥—Ä
		local GameSelectionFrame = Instance.new("Frame")
		GameSelectionFrame.Name = "GameSelectionFrame"
		GameSelectionFrame.Size = UDim2.new(0, 600, 0, 400)
		GameSelectionFrame.Position = UDim2.new(0.5, -300, 0.5, -200)
		GameSelectionFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
		GameSelectionFrame.BorderSizePixel = 0
		GameSelectionFrame.Parent = Background

		local gameCorner = Instance.new("UICorner")
		gameCorner.CornerRadius = UDim.new(0, 12)
		gameCorner.Parent = GameSelectionFrame

		local gameStroke = Instance.new("UIStroke")
		gameStroke.Color = Color3.fromRGB(100, 100, 200)
		gameStroke.Thickness = 2
		gameStroke.Parent = GameSelectionFrame

		-- –ó–∞–≥–æ–ª–æ–≤–æ–∫
		local SelectionTitle = Instance.new("TextLabel")
		SelectionTitle.Name = "SelectionTitle"
		SelectionTitle.Size = UDim2.new(1, 0, 0, 50)
		SelectionTitle.Position = UDim2.new(0, 0, 0, 0)
		SelectionTitle.BackgroundColor3 = Color3.fromRGB(40, 40, 80)
		SelectionTitle.Text = "üéÆ –í–´–ë–ï–†–ò–¢–ï –ò–ì–†–£ (FE DISABLED)"
		SelectionTitle.TextColor3 = Color3.new(1, 1, 1)
		SelectionTitle.TextScaled = true
		SelectionTitle.Font = Enum.Font.GothamBold
		SelectionTitle.Parent = GameSelectionFrame

		local selectionCorner = Instance.new("UICorner")
		selectionCorner.CornerRadius = UDim.new(0, 12)
		selectionCorner.Parent = SelectionTitle

		-- ... (–æ—Å—Ç–∞–ª—å–Ω–∞—è —á–∞—Å—Ç—å —Ñ—É–Ω–∫—Ü–∏–∏ createGameSelectionMenu –±–µ–∑ –∏–∑–º–µ–Ω–µ–Ω–∏–π)
		-- –í–ê–® –û–†–ò–ì–ò–ù–ê–õ–¨–ù–´–ô –ö–û–î –î–õ–Ø –ú–ï–ù–Æ –ò–ì–†
	end

	-- ... (–í–°–¢–ê–í–¨–¢–ï –í–°–ï –û–°–¢–ê–õ–¨–ù–´–ï –í–ê–®–ò –§–£–ù–ö–¶–ò–ò –ë–ï–ó –ò–ó–ú–ï–ù–ï–ù–ò–ô)

	print("‚ú® ANTIEZTEAM GUI V 2.5 - BEAUTIFUL –∑–∞–≥—Ä—É–∂–µ–Ω–æ!")
	print("üé® –ö—Ä–∞—Å–∏–≤—ã–π –¥–∏–∑–∞–π–Ω –¥–æ–±–∞–≤–ª–µ–Ω!")
	print("üéÆ –í—Å–µ —Ñ—É–Ω–∫—Ü–∏–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã!")
end

createGUI()
