	local ServerSideButton = createBeautifulButton("ServerSideButton", UDim2.new(0.5, 0, 0.35, 0), Color3.fromRGB(230, 150, 30), "üåé SERVERSIDE")

	-- –¢–†–ï–¢–ò–ô –°–¢–û–õ–ë–ï–¶
	local ScanButton = createBeautifulButton("ScanButton", UDim2.new(0.7, 0, 0.15, 0), Color3.fromRGB(50, 50, 180), "üîç SCAN REMOTES")
	local AntiezteamDecalButton = createBeautifulButton("AntiezteamDecalButton", UDim2.new(0.7, 0, 0.25, 0), Color3.fromRGB(200, 100, 30), "üé® ANTIEZTEAM DECAL")
	local BtoolsButton = createBeautifulButton("BtoolsButton", UDim2.new(0.7, 0, 0.35, 0), Color3.fromRGB(150, 80, 200), "üõ†Ô∏è BTOOLS")
	local SuperRingButton = createBeautifulButton("SuperRingButton", UDim2.new(0.7, 0, 0.45, 0), Color3.fromRGB(230, 80, 130), "üíç SUPER RING V4")
	local BrookhavenButton = createBeautifulButton("BrookhavenButton", UDim2.new(0.7, 0, 0.55, 0), Color3.fromRGB(30, 150, 80), "üè† BROOKHAVEN")
	local BloxFruitsButton = createBeautifulButton("BloxFruitsButton", UDim2.new(0.7, 0, 0.65, 0), Color3.fromRGB(200, 100, 30), "üçé BLOX FRUITS")

	-- ... (–í–°–¢–ê–í–¨–¢–ï –°–Æ–î–ê –í–°–ï –í–ê–®–ò –û–†–ò–ì–ò–ù–ê–õ–¨–ù–´–ï –§–£–ù–ö–¶–ò–ò –ë–ï–ó –ò–ó–ú–ï–ù–ï–ù–ò–ô)
	-- –§–£–ù–ö–¶–ò–Ø –î–õ–Ø SERVERSIDE –ö–ù–û–ü–ö–ò
	local function changeSkyColor()
		local originalAmbient = Lighting.Ambient
		local originalOutdoor = Lighting.OutdoorAmbient
		local originalBrightness = Lighting.Brightness

		Lighting.Ambient = Color3.new(1, 0, 0)
		Lighting.OutdoorAmbient = Color3.new(1, 0, 0)
		Lighting.Brightness = 2

		print("‚ú® –ù–µ–±–æ –∏–∑–º–µ–Ω–µ–Ω–æ –Ω–∞ –∫—Ä–∞—Å–Ω—ã–π!")

		wait(10)

		Lighting.Ambient = originalAmbient
		Lighting.OutdoorAmbient = originalOutdoor
		Lighting.Brightness = originalBrightness

		print("‚ú® –ù–µ–±–æ –≤–æ–∑–≤—Ä–∞—â–µ–Ω–æ –≤ –Ω–æ—Ä–º–∞–ª—å–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ")
	end

	-- –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –ú–ï–ù–Æ –í–´–ë–û–†–ê –ò–ì–†
	local function createGameSelectionMenu()
		-- –°–æ–∑–¥–∞–µ–º –∑–∞—Ç–µ–º–Ω–µ–Ω–∏–µ —Ñ–æ–Ω–∞
		local Background = Instance.new("Frame")
		Background.Name = "GameSelectionBackground"
		Background.Size = UDim2.new(1, 0, 1, 0)
		Background.Position = UDim2.new(0, 0, 0, 0)
		Background.BackgroundColor3 = Color3.new(0, 0, 0)
