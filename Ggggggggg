	ScriptBox.TextWrapped = true
	ScriptBox.Font = Enum.Font.Code
	ScriptBox.Parent = MainFrame

	local scriptCorner = Instance.new("UICorner")
	scriptCorner.CornerRadius = UDim.new(0, 8)
	scriptCorner.Parent = ScriptBox

