local buttons = {
    FEButton, NaturalDisasterButton, InfinityYieldButton,
    RemoteSpyButton, FEBypassButton, ServerSideButton,
    ScanButton, AntiezteamDecalButton, BtoolsButton,
    SuperRingButton, BrookhavenButton, BloxFruitsButton
}

for _, button in pairs(buttons) do
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 6)
    corner.Parent = button
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = button.BackgroundColor3 * 1.3
    stroke.Thickness = 1
    stroke.Parent = button
end

