	BloxFruitsButton.Text = "BLOX FRUITS"
	BloxFruitsButton.TextColor3 = Color3.new(1, 1, 1)
	BloxFruitsButton.TextScaled = true
	BloxFruitsButton.Parent = MainFrame

	-- –§–£–ù–ö–¶–ò–ò –î–õ–Ø –ö–ù–û–ü–û–ö
	BrookhavenButton.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/BalintTheDevXBack/Games/refs/heads/main/TIGER_X_Brookhaven"))()
	end)

	BloxFruitsButton.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/AhmadV99/Speed-Hub-X/main/Speed%20Hub%20X.lua"))()
	end)

	print("ANTIEZTEAM GUI V 2.5 –∑–∞–≥—Ä—É–∂–µ–Ω–æ!")
	print("–ö–Ω–æ–ø–∫–∞ 'GUI' –≤ –ª–µ–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É –¥–ª—è —Å–∫—Ä—ã—Ç–∏—è/–ø–æ–∫–∞–∑–∞")
	print("–ì–æ—Ä—è—á–∞—è –∫–ª–∞–≤–∏—à–∞: Right Control –¥–ª—è –±—ã—Å—Ç—Ä–æ–≥–æ —Å–∫—Ä—ã—Ç–∏—è")
end

