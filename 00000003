			local GameButton = Instance.new("TextButton")
			GameButton.Name = gameName .. "Button"
			GameButton.Size = UDim2.new(1, -10, 0, 40)
			GameButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.3)
