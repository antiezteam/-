		-- –û–±–Ω–æ–≤–ª—è–µ–º —Ä–∞–∑–º–µ—Ä –∫–æ–Ω—Ç–µ–Ω—Ç–∞ –≤ ScrollingFrame
		wait(0.1)
		GamesScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y)
	end

	-- –§–£–ù–ö–¶–ò–Ø –ü–ï–†–ï–ú–ï–©–ï–ù–ò–Ø –¢–û–õ–¨–ö–û –î–õ–Ø –ü–ö
	local function makeDraggable(gui)
		local dragging = false
		local dragInput, dragStart, startPos

		gui.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = gui.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		gui.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				dragInput = input
			end
		end)

		UserInputService.InputChanged:Connect(function(input)
			if input == dragInput and dragging then
				local delta = input.Position - dragStart
				gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
			end
		end)
	end

	-- –§–£–ù–ö–¶–ò–Ø –î–õ–Ø –°–ö–†–´–¢–ò–Ø/–ü–û–ö–ê–ó–ê GUI
	local function toggleGUI()
		if MainFrame.Visible then
			-- –°–∫—Ä—ã–≤–∞–µ–º GUI —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
			local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tween = TweenService:Create(MainFrame, tweenInfo, {Position = UDim2.new(0.5, -425, -1, -250)})
			tween:Play()

			wait(0.3)
			MainFrame.Visible = false
			ToggleGUIButton.Text = "GUI +"
			print("GUI —Å–∫—Ä—ã—Ç–æ")
		else
			-- –ü–æ–∫–∞–∑—ã–≤–∞–µ–º GUI —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
			MainFrame.Visible = true
			local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tween = TweenService:Create(MainFrame, tweenInfo, {Position = UDim2.new(0.5, -425, 0.5, -250)})
			tween:Play()

			ToggleGUIButton.Text = "GUI -"
			print("GUI –ø–æ–∫–∞–∑–∞–Ω–æ")
		end
	end

	-- –ì–û–†–Ø–ß–ê–Ø –ö–õ–ê–í–ò–®–ê –î–õ–Ø –°–ö–†–´–¢–ò–Ø GUI (Right Control)
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end

		if input.KeyCode == Enum.KeyCode.RightControl then
			toggleGUI()
		end
	end)

	makeDraggable(MainFrame)
	makeDraggable(TitleLabel)
	makeDraggable(ToggleGUIButton)

	-- –§–£–ù–ö–¶–ò–ò –î–õ–Ø –ö–ù–û–ü–û–ö
	ExecuteButton.MouseButton1Click:Connect(function()
		local scriptText = ScriptBox.Text
		if scriptText and scriptText ~= "" then
			print("–í—ã–ø–æ–ª–Ω—è–µ–º —Å–∫—Ä–∏–ø—Ç: " .. scriptText)
			local success, errorMessage = pcall(function()
				loadstring(scriptText)()
			end)
			if not success then
				warn("–û—à–∏–±–∫–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è —Å–∫—Ä–∏–ø—Ç–∞: " .. errorMessage)
			end
		else
			print("–í–≤–µ–¥–∏—Ç–µ —Å–∫—Ä–∏–ø—Ç –≤ —Ç–µ–∫—Å—Ç–æ–≤–æ–µ –ø–æ–ª–µ!")
		end
	end)

	ServerSideButton.MouseButton1Click:Connect(function()
		print("ServerSide: –ú–µ–Ω—è–µ–º —Ü–≤–µ—Ç –Ω–µ–±–∞ –¥–ª—è –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤!")
		changeSkyColor()
	end)

	FEButton.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://pastebin.com/raw/XJeBNe0s"))()
	end)

	NaturalDisasterButton.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/antiezteam/-/refs/heads/main/fwwafwfaf"))()
	end)

	InfinityYieldButton.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end)

	RemoteSpyButton.MouseButton1Click:Connect(function()
		print("REMOTE SPY –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!")
		loadstring(game:HttpGet("https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpySource.lua"))()
	end)

	FEBypassButton.MouseButton1Click:Connect(function()
		print("FE BYPASS –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!")
		loadstring(game:HttpGet("https://pastebin.com/raw/1234567890"))()
	end)

	ScanButton.MouseButton1Click:Connect(function()
		print("SCAN REMOTES –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!")
		for i,v in pairs(game:GetDescendants()) do
			if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
				print("–ù–∞–π–¥–µ–Ω Remote: " .. v:GetFullName())
			end
		end
	end)

	AntiezteamDecalButton.MouseButton1Click:Connect(function()
		print("ANTIEZTEAM DECAL –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!")
		local decalId = "rbxassetid://1234567890"
		for i,v in pairs(workspace:GetDescendants()) do
			if v:IsA("Part") and v.Name == "Baseplate" then
				local decal = Instance.new("Decal")
				decal.Texture = decalId
				decal.Face = Enum.NormalId.Top
				decal.Parent = v
			end
		end
	end)

	BtoolsButton.MouseButton1Click:Connect(function()
		print("BTOOLS –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω—ã!")
		local tool1 = Instance.new("HopperBin")
		tool1.BinType = Enum.BinType.Hammer
		tool1.Parent = player.Backpack

		local tool2 = Instance.new("HopperBin")
		tool2.BinType = Enum.BinType.Clone
		tool2.Parent = player.Backpack

		local tool3 = Instance.new("HopperBin")
		tool3.BinType = Enum.BinType.Grab
		tool3.Parent = player.Backpack
	end)

	SuperRingButton.MouseButton1Click:Connect(function()
		print("SUPER RING V4 –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!")
		loadstring(game:HttpGet("https://raw.githubusercontent.com/antiezteam/-/refs/heads/main/super%20ring%20part%20v4"))()
	end)

	SelectGameButton.MouseButton1Click:Connect(function()
		print("–û—Ç–∫—Ä—ã–≤–∞–µ–º –º–µ–Ω—é –≤—ã–±–æ—Ä–∞ –∏–≥—Ä...")
		createGameSelectionMenu()
	end)

	ToggleGUIButton.MouseButton1Click:Connect(function()
		toggleGUI()
	end)

	-- –ö–ù–û–ü–ö–ê BROOKHAVEN
	local BrookhavenButton = Instance.new("TextButton")
	BrookhavenButton.Name = "BrookhavenButton"
	BrookhavenButton.Size = UDim2.new(0.18, 0, 0, 30)
	BrookhavenButton.Position = UDim2.new(0.7, 0, 0.55, 0)
	BrookhavenButton.BackgroundColor3 = Color3.new(0.1, 0.6, 0.3)
	BrookhavenButton.Text = "BROOKHAVEN"
	BrookhavenButton.TextColor3 = Color3.new(1, 1, 1)
	BrookhavenButton.TextScaled = true
