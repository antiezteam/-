-- –î–û–ë–ê–í–õ–Ø–ï–ú –ö–†–ê–°–ò–í–´–ô –î–ò–ó–ê–ô–ù –ö–ù–û–ü–ö–ê–ú
local buttons = {FEButton, FEBypassButton, RemoteSpyButton, AntiGUIButton, BtoolsButton, InfinityYieldButton}

for _, button in pairs(buttons) do
	-- –ó–∞–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–µ —É–≥–ª—ã
	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(0, 8)
	corner.Parent = button
	
	-- –û–±–≤–æ–¥–∫–∞
	local stroke = Instance.new("UIStroke")
	stroke.Color = button.BackgroundColor3 * 1.3
	stroke.Thickness = 2
	stroke.Parent = button
	
	-- –≠—Ñ—Ñ–µ–∫—Ç –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏
	button.MouseEnter:Connect(function()
		local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(button, tweenInfo, {BackgroundColor3 = button.BackgroundColor3 * 1.2})
		tween:Play()
	end)
	
	button.MouseLeave:Connect(function()
		local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(button, tweenInfo, {BackgroundColor3 = button.BackgroundColor3 / 1.2})
		tween:Play()
	end)
end"print("ANTIEZTEAM GUI V 2.5 –∑–∞–≥—Ä—É–∂–µ–Ω–æ!")"

		print("‚úÖ –ó–∞–≥—Ä—É–∂–µ–Ω–∞ –ü–ö –≤–µ—Ä—Å–∏—è GUI")
	end

	-- –û–ë–©–ò–ï –§–£–ù–ö–¶–ò–ò –î–õ–Ø –û–ë–ï–ò–• –í–ï–†–°–ò–ô
	local function changeSkyColor()
		local originalAmbient = Lighting.Ambient
		local originalOutdoor = Lighting.OutdoorAmbient
		local originalBrightness = Lighting.Brightness

		Lighting.Ambient = Color3.new(1, 0, 0)
		Lighting.OutdoorAmbient = Color3.new(1, 0, 0)
		Lighting.Brightness = 2

		print("–ù–µ–±–æ –∏–∑–º–µ–Ω–µ–Ω–æ –Ω–∞ –∫—Ä–∞—Å–Ω—ã–π!")

		wait(10)

		Lighting.Ambient = originalAmbient
		Lighting.OutdoorAmbient = originalOutdoor
		Lighting.Brightness = originalBrightness

		print("–ù–µ–±–æ –≤–æ–∑–≤—Ä–∞—â–µ–Ω–æ –≤ –Ω–æ—Ä–º–∞–ª—å–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ")
	end

	local function createGameSelectionMenu()
		-- –í–∞—à–∞ —Ñ—É–Ω–∫—Ü–∏—è –≤—ã–±–æ—Ä–∞ –∏–≥—Ä (—Ç–∞–∫–∞—è –∂–µ –∫–∞–∫ –≤ –æ—Ä–∏–≥–∏–Ω–∞–ª–µ)
		-- ... (–≤—Å—Ç–∞–≤—å—Ç–µ –≤–∞—à—É —Ñ—É–Ω–∫—Ü–∏—é createGameSelectionMenu —Å—é–¥–∞)
	end

	print("üéÆ ANTIEZTEAM GUI V 2.8 –∑–∞–≥—Ä—É–∂–µ–Ω–æ!")
	print("üì± –ü–ª–∞—Ç—Ñ–æ—Ä–º–∞: " .. platform)
end

createGUI()
