RemoteSpyButton.Parent = MainFrame

-- –í–¢–û–†–û–ô –°–¢–û–õ–ë–ï–¶ –ö–ù–û–ü–û–ö –î–õ–Ø –ü–ö
local AntiGUIButton = Instance.new("TextButton")
AntiGUIButton.Name = "AntiGUIButton"
AntiGUIButton.Size = UDim2.new(0.18, 0, 0, 35)
AntiGUIButton.Position = UDim2.new(0.5, 0, 0.15, 0)
