		ButtonsScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y)

		-- –ö–Ω–æ–ø–∫–∞ –æ—Ç–∫—Ä—ã—Ç–∏—è GUI –¥–ª—è –º–æ–±–∏–ª—å–Ω–æ–π –≤–µ—Ä—Å–∏–∏
		local OpenGUIButton = Instance.new("TextButton")
		OpenGUIButton.Name = "OpenGUIButton"
		OpenGUIButton.Size = UDim2.new(0, 80, 0, 80)
		OpenGUIButton.Position = UDim2.new(0, 10, 0.5, -40)
		OpenGUIButton.BackgroundColor3 = Color3.fromRGB(40, 40, 80)
		OpenGUIButton.Text = "üì±\nGUI"
		OpenGUIButton.TextColor3 = Color3.new(1, 1, 1)
		OpenGUIButton.TextScaled = true
		OpenGUIButton.Visible = not MainFrame.Visible
		OpenGUIButton.ZIndex = 10
		OpenGUIButton.Parent = ScreenGui

		local openCorner = Instance.new("UICorner")
		openCorner.CornerRadius = UDim.new(1, 0)
		openCorner.Parent = OpenGUIButton

		-- –§—É–Ω–∫—Ü–∏–∏ –¥–ª—è –º–æ–±–∏–ª—å–Ω—ã—Ö –∫–Ω–æ–ø–æ–∫
		OpenGUIButton.MouseButton1Click:Connect(function()
			MainFrame.Visible = true
			OpenGUIButton.Visible = false
		end)

		CloseButton.MouseButton1Click:Connect(function()
			MainFrame.Visible = false
			OpenGUIButton.Visible = true
		end)

		print("‚úÖ –ó–∞–≥—Ä—É–∂–µ–Ω–∞ –ú–û–ë–ò–õ–¨–ù–ê–Ø –≤–µ—Ä—Å–∏—è GUI")

	else
		-- üñ•Ô∏è –ü–ö –í–ï–†–°–ò–Ø (–í–ê–® –û–†–ò–ì–ò–ù–ê–õ–¨–ù–´–ô –°–ö–†–ò–ü–¢)
		-- –ú–ò–ù–ò-–ö–ù–û–ü–ö–ê GUI (–¥–ª—è —Å–∫—Ä—ã—Ç–∏—è/–ø–æ–∫–∞–∑–∞)
		local ToggleGUIButton = Instance.new("TextButton")
		ToggleGUIButton.Name = "ToggleGUIButton"
		ToggleGUIButton.Size = UDim2.new(0, 60, 0, 30)
		ToggleGUIButton.Position = UDim2.new(0, 10, 0, 10)
		ToggleGUIButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.4)
		ToggleGUIButton.BorderSizePixel = 1
		ToggleGUIButton.BorderColor3 = Color3.new(0.5, 0.5, 0.5)
		ToggleGUIButton.Text = "GUI"
		ToggleGUIButton.TextColor3 = Color3.new(1, 1, 1)
		ToggleGUIButton.TextScaled = true
		ToggleGUIButton.ZIndex = 10
		ToggleGUIButton.Parent = ScreenGui

		-- –ì–û–†–ò–ó–û–ù–¢–ê–õ–¨–ù–´–ô –ì–õ–ê–í–ù–´–ô –§–†–ï–ô–ú
		local MainFrame = Instance.new("Frame")
		MainFrame.Name = "MainFrame"
		MainFrame.Size = UDim2.new(0, 850, 0, 500)
		MainFrame.Position = UDim2.new(0.5, -425, 0.5, -250)
		MainFrame.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
		MainFrame.BorderSizePixel = 2
		MainFrame.BorderColor3 = Color3.new(0.3, 0.3, 0.3)
		MainFrame.Visible = true
		MainFrame.Parent = ScreenGui

		local TitleLabel = Instance.new("TextLabel")
		TitleLabel.Name = "TitleLabel"
		TitleLabel.Size = UDim2.new(1, 0, 0, 40)
		TitleLabel.Position = UDim2.new(0, 0, 0, 0)
		TitleLabel.BackgroundColor3 = Color3.new(0.2, 0.2, 0.4)
		TitleLabel.Text = "ANTIEZTEAM GUI V 2.8 - PC VERSION"
		TitleLabel.TextColor3 = Color3.new(1, 1, 1)
		TitleLabel.TextScaled = true
		TitleLabel.Parent = MainFrame

		-- –ü–ï–†–í–´–ô –°–¢–û–õ–ë–ï–¶ –ö–ù–û–ü–û–ö –î–õ–Ø –ü–ö
local FEButton = Instance.new("TextButton")
FEButton.Name = "FEButton"
FEButton.Size = UDim2.new(0.18, 0, 0, 35)
FEButton.Position = UDim2.new(0.3, 0, 0.15, 0)
FEButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
FEButton.Text = "üîß FE CADUCUS"
FEButton.TextColor3 = Color3.new(1, 1, 1)
FEButton.TextScaled = true
FEButton.Font = Enum.Font.Gotham
FEButton.Parent = MainFrame

local FEBypassButton = Instance.new("TextButton")
FEBypassButton.Name = "FEBypassButton"
FEBypassButton.Size = UDim2.new(0.18, 0, 0, 35)
FEBypassButton.Position = UDim2.new(0.3, 0, 0.25, 0)
FEBypassButton.BackgroundColor3 = Color3.fromRGB(30, 180, 200)
FEBypassButton.Text = "üõ°Ô∏è FE BYPASS"
FEBypassButton.TextColor3 = Color3.new(1, 1, 1)
FEBypassButton.TextScaled = true
FEBypassButton.Font = Enum.Font.Gotham
FEBypassButton.Parent = MainFrame

local RemoteSpyButton = Instance.new("TextButton")
RemoteSpyButton.Name = "RemoteSpyButton"
RemoteSpyButton.Size = UDim2.new(0.18, 0, 0, 35)
RemoteSpyButton.Position = UDim2.new(0.3, 0, 0.35, 0)
RemoteSpyButton.BackgroundColor3 = Color3.fromRGB(180, 80, 220)
RemoteSpyButton.Text = "üëÅÔ∏è REMOTE SPY"
RemoteSpyButton.TextColor3 = Color3.new(1, 1, 1)
RemoteSpyButton.TextScaled = true
